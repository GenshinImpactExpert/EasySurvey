{"ast":null,"code":"export default {\n  name: \"FPages\",\n  data() {\n    return {\n      tableData: [],\n      // 所有的数据\n      pageNum: 1,\n      // 当前的页码\n      pageSize: 4,\n      // 每页显示的个数\n      total: 0,\n      name: null,\n      fromVisible: false,\n      form: {},\n      user: JSON.parse(localStorage.getItem('xm-user') || '{}'),\n      rules: {\n        name: [{\n          required: true,\n          message: '请输入名称',\n          trigger: 'blur'\n        }],\n        descr: [{\n          required: true,\n          message: '请输入简介',\n          trigger: 'blur'\n        }]\n      }\n    };\n  },\n  created() {\n    this.load(1);\n  },\n  methods: {\n    design(pageId) {\n      window.open('/front/design?pageId=' + pageId);\n    },\n    handleAdd() {\n      // 新增数据\n      this.form = {}; // 新增数据的时候清空数据\n      this.fromVisible = true; // 打开弹窗\n    },\n\n    handleEdit(row) {\n      // 编辑数据\n      this.form = JSON.parse(JSON.stringify(row)); // 给form对象赋值  注意要深拷贝数据\n      this.fromVisible = true; // 打开弹窗\n    },\n\n    save() {\n      // 保存按钮触发的逻辑  它会触发新增或者更新\n      this.$refs.formRef.validate(valid => {\n        if (valid) {\n          this.$request({\n            url: this.form.id ? '/pages/update' : '/pages/add',\n            method: this.form.id ? 'PUT' : 'POST',\n            data: this.form\n          }).then(res => {\n            if (res.code === '200') {\n              // 表示成功保存\n              this.$message.success('保存成功');\n              this.load(1);\n              this.fromVisible = false;\n            } else {\n              this.$message.error(res.msg); // 弹出错误的信息\n            }\n          });\n        }\n      });\n    },\n\n    del(pageId) {\n      // 单个删除\n      this.$confirm('您确定删除吗？', '确认删除', {\n        type: \"warning\"\n      }).then(response => {\n        this.$request.delete('/pages/delete/' + pageId).then(res => {\n          if (res.code === '200') {\n            // 表示操作成功\n            this.$message.success('操作成功');\n            this.load(1);\n          } else {\n            this.$message.error(res.msg); // 弹出错误的信息\n          }\n        });\n      }).catch(() => {});\n    },\n    load(pageNum) {\n      // 分页查询\n      if (pageNum) this.pageNum = pageNum;\n      this.$request.get('/pages/selectPage', {\n        params: {\n          pageNum: this.pageNum,\n          pageSize: this.pageSize,\n          name: this.name\n        }\n      }).then(res => {\n        this.tableData = res.data?.list;\n        this.total = res.data?.total;\n        console.log(res.data.list);\n      });\n    },\n    reset() {\n      this.name = null;\n      this.load(1);\n    },\n    handleCurrentChange(pageNum) {\n      this.load(pageNum);\n    },\n    handleImgSuccess(res) {\n      this.form.img = res.data;\n    }\n  }\n};","map":{"version":3,"names":["name","data","tableData","pageNum","pageSize","total","fromVisible","form","user","JSON","parse","localStorage","getItem","rules","required","message","trigger","descr","created","load","methods","design","pageId","window","open","handleAdd","handleEdit","row","stringify","save","$refs","formRef","validate","valid","$request","url","id","method","then","res","code","$message","success","error","msg","del","$confirm","type","response","delete","catch","get","params","list","console","log","reset","handleCurrentChange","handleImgSuccess","img"],"sources":["src/views/front/Pages.vue"],"sourcesContent":["<script>\r\nexport default {\r\n  name: \"FPages\",\r\n  data(){\r\n    return{\r\n      tableData: [],  // 所有的数据\r\n      pageNum: 1,   // 当前的页码\r\n      pageSize: 4,  // 每页显示的个数\r\n      total: 0,\r\n      name:null,\r\n      fromVisible:false,\r\n      form:{},\r\n      user: JSON.parse(localStorage.getItem('xm-user') || '{}'),\r\n\r\n      rules: {\r\n        name: [\r\n          {required: true, message: '请输入名称', trigger: 'blur'},\r\n        ],\r\n        descr: [\r\n          {required: true, message: '请输入简介', trigger: 'blur'},\r\n        ]\r\n      },\r\n    }\r\n  },\r\n  created() {\r\n    this.load(1)\r\n  },\r\n  methods:{\r\n    design(pageId){\r\n      window.open('/front/design?pageId='+pageId)\r\n    },\r\n    handleAdd() {   // 新增数据\r\n      this.form = {}  // 新增数据的时候清空数据\r\n      this.fromVisible = true   // 打开弹窗\r\n    },\r\n    handleEdit(row) {   // 编辑数据\r\n      this.form = JSON.parse(JSON.stringify(row))  // 给form对象赋值  注意要深拷贝数据\r\n      this.fromVisible = true   // 打开弹窗\r\n    },\r\n    save() {   // 保存按钮触发的逻辑  它会触发新增或者更新\r\n      this.$refs.formRef.validate((valid) => {\r\n        if (valid) {\r\n          this.$request({\r\n            url: this.form.id ? '/pages/update' : '/pages/add',\r\n            method: this.form.id ? 'PUT' : 'POST',\r\n            data: this.form\r\n          }).then(res => {\r\n            if (res.code === '200') {  // 表示成功保存\r\n              this.$message.success('保存成功')\r\n              this.load(1)\r\n              this.fromVisible = false\r\n            } else {\r\n              this.$message.error(res.msg)  // 弹出错误的信息\r\n            }\r\n          })\r\n        }\r\n      })\r\n    },\r\n    del(pageId) {   // 单个删除\r\n      this.$confirm('您确定删除吗？', '确认删除', {type: \"warning\"}).then(response => {\r\n        this.$request.delete('/pages/delete/' + pageId).then(res => {\r\n          if (res.code === '200') {   // 表示操作成功\r\n            this.$message.success('操作成功')\r\n            this.load(1)\r\n          } else {\r\n            this.$message.error(res.msg)  // 弹出错误的信息\r\n          }\r\n        })\r\n      }).catch(() => {\r\n      })\r\n    },\r\n    load(pageNum) {  // 分页查询\r\n      if (pageNum) this.pageNum = pageNum\r\n      this.$request.get('/pages/selectPage', {\r\n        params: {\r\n          pageNum: this.pageNum,\r\n          pageSize: this.pageSize,\r\n          name: this.name,\r\n\r\n\r\n        }\r\n      }).then(res => {\r\n        this.tableData = res.data?.list\r\n        this.total = res.data?.total\r\n        console.log(res.data.list)\r\n      })\r\n    },\r\n    reset() {\r\n      this.name = null\r\n\r\n      this.load(1)\r\n    },\r\n    handleCurrentChange(pageNum) {\r\n      this.load(pageNum)\r\n    },\r\n    handleImgSuccess(res){\r\n      this.form.img=res.data\r\n    }\r\n  }\r\n}\r\n</script>\r\n\r\n<template>\r\n  <div class=\"main-content\">\r\n    <div style=\"display: flex;margin-bottom: 10px\">\r\n      <div style=\"flex: 1;\">\r\n        <el-button type=\"primary\" size=\"medium\" class=\"btn-green\" @click=\"handleAdd\">创建新的问卷</el-button>\r\n      </div>\r\n      <div>\r\n        <el-input v-model=\"name\" style=\"width: 300px;margin-right: 10px\" size=\"medium\" placeholder=\"请输入名称搜索问卷\" clearable></el-input>\r\n        <el-button type=\"primary\" size=\"medium\" class=\"btn-green\" @click=\"load(1)\">搜 索</el-button>\r\n      </div>\r\n    </div>\r\n\r\n    <div style=\"margin-bottom: 10px\">\r\n      <div class=\"card\" v-for=\"item in tableData\" :key=\"item.id\" style=\"margin-bottom: 5px\">\r\n        <div style=\"display: flex\">\r\n          <div style=\"flex: 1;width: 0\">\r\n           <div style=\"display: flex;align-items: center;margin-bottom: 10px\">\r\n             <el-tag v-if=\"item.saved==='否'\" type=\"warning\" >未发布</el-tag>\r\n             <el-tag v-if=\"item.saved==='是'\" type=\"success\" >已发布</el-tag>\r\n             <span style=\"font-size:20px;margin-left: 10px\">{{item.name}}</span>\r\n           </div>\r\n\r\n            <div>\r\n              <el-button type=\"text\" style=\"color:#2a68c9;font-size: 16px\" @click=\"handleEdit(item)\"><i class=\"el-icon-edit\"></i> 编辑</el-button>\r\n              <el-button type=\"text\" style=\"color:#2a68c9;font-size: 16px\" @click=\"design(item.id)\"><i class=\"el-icon-setting\"></i> 设计</el-button>\r\n              <el-button type=\"text\" style=\"color:#2a68c9;font-size: 16px\"><i class=\"el-icon-document-copy\"></i> 复制</el-button>\r\n              <el-button type=\"text\" style=\"color:#2a68c9;font-size: 16px\"><i class=\"el-icon-share\"></i> 分享</el-button>\r\n              <el-button type=\"text\" style=\"color:#fc4b4b;font-size: 16px\" v-if=\"item.open==='否'\" @click=\"del(item.id)\"><i class=\"el-icon-delete\" ></i> 删除</el-button>\r\n              <el-button type=\"text\" style=\"color:orange;font-size: 16px\"><i class=\"el-icon-s-marketing\"></i> 数据统计</el-button>\r\n            </div>\r\n          </div>\r\n          <div style=\"width: 100px\">\r\n            <img :src=\"item.img\" style=\"width: 100px;height: 66.67px;border-radius: 5px;display: block\">\r\n          </div>\r\n        </div>\r\n      </div>\r\n    </div>\r\n\r\n    <div class=\"pagination\" v-if=\"total\">\r\n      <el-pagination\r\n          background\r\n          @current-change=\"handleCurrentChange\"\r\n          :current-page=\"pageNum\"\r\n\r\n          :page-size=\"pageSize\"\r\n          layout=\"total, prev, pager, next\"\r\n          :total=\"total\">\r\n      </el-pagination>\r\n    </div>\r\n\r\n    <el-dialog title=\"信息\" :visible.sync=\"fromVisible\" width=\"40%\" :close-on-click-modal=\"false\" destroy-on-close>\r\n      <el-form :model=\"form\" label-width=\"100px\" style=\"padding-right: 50px\" :rules=\"rules\" ref=\"formRef\">\r\n        <el-form-item label=\"模板名称\" prop=\"name\">\r\n          <el-input v-model=\"form.name\" placeholder=\"模板名称\"></el-input>\r\n        </el-form-item>\r\n        <el-form-item label=\"模板介绍\" prop=\"descr\">\r\n          <el-input v-model=\"form.descr\" placeholder=\"模板介绍\"></el-input>\r\n        </el-form-item>\r\n        <el-form-item label=\"封面图\" prop=\"img\">\r\n          <el-upload\r\n              :action=\"$baseUrl + '/files/upload'\"\r\n              :headers=\"{ token: user.token }\"\r\n              list-type=\"picture\"\r\n              :on-success=\"handleImgSuccess\"\r\n          >\r\n            <el-button type=\"primary\">上传</el-button>\r\n          </el-upload>\r\n        </el-form-item>\r\n        <el-form-item label=\"是否公开\" prop=\"open\" v-if=\"user.role==='ADMIN'\">\r\n          <el-radio-group v-model=\"form.open\">\r\n            <el-radio label=\"是\"></el-radio>\r\n            <el-radio label=\"否\"></el-radio>\r\n          </el-radio-group>\r\n        </el-form-item>\r\n      </el-form>\r\n      <div slot=\"footer\" class=\"dialog-footer\">\r\n        <el-button @click=\"fromVisible = false\">取 消</el-button>\r\n        <el-button type=\"primary\" @click=\"save\">确 定</el-button>\r\n      </div>\r\n    </el-dialog>\r\n  </div>\r\n</template>\r\n\r\n<style scoped>\r\n\r\n</style>"],"mappings":"AACA;EACAA,IAAA;EACAC,KAAA;IACA;MACAC,SAAA;MAAA;MACAC,OAAA;MAAA;MACAC,QAAA;MAAA;MACAC,KAAA;MACAL,IAAA;MACAM,WAAA;MACAC,IAAA;MACAC,IAAA,EAAAC,IAAA,CAAAC,KAAA,CAAAC,YAAA,CAAAC,OAAA;MAEAC,KAAA;QACAb,IAAA,GACA;UAAAc,QAAA;UAAAC,OAAA;UAAAC,OAAA;QAAA,EACA;QACAC,KAAA,GACA;UAAAH,QAAA;UAAAC,OAAA;UAAAC,OAAA;QAAA;MAEA;IACA;EACA;EACAE,QAAA;IACA,KAAAC,IAAA;EACA;EACAC,OAAA;IACAC,OAAAC,MAAA;MACAC,MAAA,CAAAC,IAAA,2BAAAF,MAAA;IACA;IACAG,UAAA;MAAA;MACA,KAAAlB,IAAA;MACA,KAAAD,WAAA;IACA;;IACAoB,WAAAC,GAAA;MAAA;MACA,KAAApB,IAAA,GAAAE,IAAA,CAAAC,KAAA,CAAAD,IAAA,CAAAmB,SAAA,CAAAD,GAAA;MACA,KAAArB,WAAA;IACA;;IACAuB,KAAA;MAAA;MACA,KAAAC,KAAA,CAAAC,OAAA,CAAAC,QAAA,CAAAC,KAAA;QACA,IAAAA,KAAA;UACA,KAAAC,QAAA;YACAC,GAAA,OAAA5B,IAAA,CAAA6B,EAAA;YACAC,MAAA,OAAA9B,IAAA,CAAA6B,EAAA;YACAnC,IAAA,OAAAM;UACA,GAAA+B,IAAA,CAAAC,GAAA;YACA,IAAAA,GAAA,CAAAC,IAAA;cAAA;cACA,KAAAC,QAAA,CAAAC,OAAA;cACA,KAAAvB,IAAA;cACA,KAAAb,WAAA;YACA;cACA,KAAAmC,QAAA,CAAAE,KAAA,CAAAJ,GAAA,CAAAK,GAAA;YACA;UACA;QACA;MACA;IACA;;IACAC,IAAAvB,MAAA;MAAA;MACA,KAAAwB,QAAA;QAAAC,IAAA;MAAA,GAAAT,IAAA,CAAAU,QAAA;QACA,KAAAd,QAAA,CAAAe,MAAA,oBAAA3B,MAAA,EAAAgB,IAAA,CAAAC,GAAA;UACA,IAAAA,GAAA,CAAAC,IAAA;YAAA;YACA,KAAAC,QAAA,CAAAC,OAAA;YACA,KAAAvB,IAAA;UACA;YACA,KAAAsB,QAAA,CAAAE,KAAA,CAAAJ,GAAA,CAAAK,GAAA;UACA;QACA;MACA,GAAAM,KAAA,QACA;IACA;IACA/B,KAAAhB,OAAA;MAAA;MACA,IAAAA,OAAA,OAAAA,OAAA,GAAAA,OAAA;MACA,KAAA+B,QAAA,CAAAiB,GAAA;QACAC,MAAA;UACAjD,OAAA,OAAAA,OAAA;UACAC,QAAA,OAAAA,QAAA;UACAJ,IAAA,OAAAA;QAGA;MACA,GAAAsC,IAAA,C