{"ast":null,"code":"export default {\n  name: \"FPages\",\n  data() {\n    return {\n      name: ''\n    };\n  },\n  methods: {\n    load(pageNum) {\n      // 分页查询\n      if (pageNum) this.pageNum = pageNum;\n      this.$request.get('/pages/selectPage', {\n        params: {\n          pageNum: this.pageNum,\n          pageSize: this.pageSize,\n          name: this.name,\n          userName: this.userName\n        }\n      }).then(res => {\n        this.tableData = res.data?.list;\n        this.total = res.data?.total;\n        console.log(res.data.list);\n      });\n    },\n    reset() {\n      this.name = null;\n      this.userName = null;\n      this.load(1);\n    },\n    handleCurrentChange(pageNum) {\n      this.load(pageNum);\n    }\n  }\n};","map":{"version":3,"names":["name","data","methods","load","pageNum","$request","get","params","pageSize","userName","then","res","tableData","list","total","console","log","reset","handleCurrentChange"],"sources":["src/views/front/Pages.vue"],"sourcesContent":["<script>\r\nexport default {\r\n  name: \"FPages\",\r\n  data(){\r\n    return{\r\n      name:''\r\n    }\r\n  },\r\n  methods:{\r\n    load(pageNum) {  // 分页查询\r\n      if (pageNum) this.pageNum = pageNum\r\n      this.$request.get('/pages/selectPage', {\r\n        params: {\r\n          pageNum: this.pageNum,\r\n          pageSize: this.pageSize,\r\n          name: this.name,\r\n          userName: this.userName,\r\n\r\n        }\r\n      }).then(res => {\r\n        this.tableData = res.data?.list\r\n        this.total = res.data?.total\r\n        console.log(res.data.list)\r\n      })\r\n    },\r\n    reset() {\r\n      this.name = null\r\n      this.userName=null\r\n      this.load(1)\r\n    },\r\n    handleCurrentChange(pageNum) {\r\n      this.load(pageNum)\r\n    },\r\n  }\r\n}\r\n</script>\r\n\r\n<template>\r\n  <div class=\"main-content\">\r\n    <div style=\"display: flex\">\r\n      <div style=\"flex: 1;\">\r\n        <el-button type=\"primary\" size=\"m